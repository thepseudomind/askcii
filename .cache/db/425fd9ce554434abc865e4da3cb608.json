c/components/Product/Product.js","original":{"line":23,"column":4}},{"generated":{"line":56,"column":0},"source":"../src/components/Product/Product.js","original":{"line":35,"column":1}},{"generated":{"line":56,"column":1},"source":"../src/components/Product/Product.js","original":{"line":4,"column":0}},{"name":"Product","generated":{"line":58,"column":15},"source":"../src/components/Product/Product.js","original":{"line":37,"column":15}}],"sources":{"../src/components/Product/Product.js":"import React from 'react';\nimport './Product.scss';\n\nconst Product =({status, face, size, date, price})=>{\n    const dateToDisplay = (fetchedDate)=>{\n        const dateDifference = Math.abs(new Date() - new Date(fetchedDate));\n        const dateInDays = Math.floor(dateDifference / (1000 * 60 * 60 * 24))\n        if(dateInDays <= 7 && dateInDays > 1){\n            return `${dateInDays} days ago`;\n        }else if(dateInDays === 1){\n            return `${dateInDays} day ago`;;\n        }else if(dateInDays === 0){\n            return 'Today';\n        }else{\n            return new Date(fetchedDate).toDateString();\n        }\n    }\n\n    const formatPrice = (price)=>{\n        return `$${(price/100).toFixed(2)}`;\n    }\n\n    return (\n        <div className={`product${(status) ? ' active' : ''}`}>\n            <div className=\"product__icon\">\n                <span style={{fontSize: `${size}px`}}>{face}</span>\n            </div>\n            <div className=\"product__content\">\n                <p className=\"product__price\">{formatPrice(price)}</p>\n                <p>{`Size: ${size}px`}</p>\n                <p className=\"product__date\">{dateToDisplay(date)}</p>\n            </div>\n        </div>\n    );\n}\n\nexport default Product;"},"lineCount":null}},"error":null,"hash":"4929b2923b68b160a55ad8ca45c04294","cacheData":{"env":{}}}